. /lib/functions/uci-defaults.sh
. /lib/functions/system.sh

setup_me_booty()
{
	local board="$1"

	case "$board" in
	netgear,wax610)
		ucidef_set_interface_lan "eth0"
		;;
	*)
		ucidef_set_interface_lan "eth0" "dhcp"
		;;
	esac
}

board_config_update
board=$(board_name)
setup_me_booty $board
board_config_flush

exit 0
